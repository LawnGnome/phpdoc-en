<refentry xml:id="swftext.addutf8string" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>SWFText->addUTF8String</refname>
  <refpurpose>Writes the given text into this SWFText object at the current pen position,
 using the current font, height, spacing, and color</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <classsynopsis>
   <ooclass><classname>SWFText</classname></ooclass>
   <methodsynopsis>
    <type>void</type><methodname>addUTF8String</methodname>
    <methodparam><type>string</type><parameter>text</parameter></methodparam>
   </methodsynopsis>
  </classsynopsis>

  &warn.undocumented.func;

 </refsect1>
