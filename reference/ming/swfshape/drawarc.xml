<refentry xml:id="swfshape.drawarc" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>SWFShape::drawArc</refname>
  <refpurpose>Draws an arc of radius r centered at the current location, from angle startAngle to angle endAngle measured clockwise from 12 o'clock</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>void</type><methodname>SWFShape::drawArc</methodname>
   <methodparam><type>float</type><parameter>r</parameter></methodparam>
   <methodparam><type>float</type><parameter>startAngle</parameter></methodparam>
   <methodparam><type>float</type><parameter>endAngle</parameter></methodparam>
  </methodsynopsis>

  &warn.undocumented.func;

 </refsect1>
